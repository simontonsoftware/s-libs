    "expect-type": "^1.1.0",
    "glob": "^11.0.0",
    "jasmine-core": "~5.7.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "lodash-es": "^4.17.21",
    "ng-packagr": "^20.0.0",
    "prettier": "^3.0.3",
    "sassdoc": "^2.7.4",
    "source-map-explorer": "^2.5.3",
    "standard-version": "^9.5.0",
    "tsx": "^4.20.3",
    "typedoc": "^0.28.5",
    "typescript": "~5.8.2",
    "typescript-eslint": "8.33.1"
